- @quizzes.each do |quiz|
  .jumbotron
    .container
      %h2= quiz.title
      %p
        = link_to 'Show', teachers_quiz_path(quiz.id), { :class=>"btn btn-success" }
        = link_to 'Edit', edit_teachers_quiz_path(quiz.id), { :class=>"btn btn-success" }
        = link_to 'Destroy', teachers_quiz_path(quiz.id), { method: :delete, :class=>"btn btn-success" }
= link_to 'New Quiz', new_teachers_quiz_path, { :class=>"btn btn-success" }        